{
  "name": "rn-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android:dev": "cross-env ENVFILE=.env.dev react-native run-android",
    "android:staging": "cd ./android && cross-env ENVFILE=.env.staging ./gradlew app:assembleRelease",
    "android:prod": "cd ./android && cross-env ENVFILE=.env.prod ./gradlew app:assembleRelease",
    "ios": "react-native run-ios",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "commit": "git-cz",
    "tsc": "tsc -p tsconfig.json",
    "pod": "cd ios && pod install --verbose --no-repo-update && cd ..",
    "postinstall": "npx husky install && npx patch-package",
    "prettier": "prettier --write --cache --cache-strategy metadata \"**/*.{tsx,ts}\"",
    "clean": "react-native-clean-project",
    "svg-init": "npx iconfont-rn"
  },
  "dependencies": {
    "@byron-react-native/refresh-control": "^1.2.5",
    "@gorhom/bottom-sheet": "^4",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-navigation/bottom-tabs": "^6.5.1",
    "@react-navigation/devtools": "^6.0.15",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/native-stack": "^6.9.6",
    "@reduxjs/toolkit": "^1.9.1",
    "@shopify/flash-list": "^1.4.1",
    "@shopify/react-native-skia": "^0.1.172",
    "@shopify/restyle": "^2.3.0",
    "ahooks": "^3.7.2",
    "axios": "^1.3.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.1",
    "global": "^4.4.0",
    "lodash-es": "^4.17.21",
    "native-base": "^3.4.25",
    "rc-field-form": "^1.27.4",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-bootsplash": "^4.4.1",
    "react-native-config": "^1.4.11",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-mmkv": "^2.5.1",
    "react-native-modal": "^13.0.1",
    "react-native-pager-view": "^6.1.2",
    "react-native-picker": "^4.3.7",
    "react-native-reanimated": "^2.14.2",
    "react-native-reanimated-carousel": "^3.3.0",
    "react-native-restart": "^0.0.24",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-svg": "^13.6.0",
    "react-native-tab-view": "^3.3.4",
    "react-native-webview": "^11.26.1",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "wrn-echarts": "^0.1.3",
    "zustand": "^4.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^26.0.23",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-import": "^1.13.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "^4.2.6",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "eslint": "^7.32.0",
    "husky": "^8.0.2",
    "jest": "^26.6.3",
    "lint-staged": "^13.1.0",
    "metro-react-native-babel-preset": "0.72.3",
    "npm": "^9.2.0",
    "react-native-clean-project": "^4.0.1",
    "react-native-flipper": "0.176.1",
    "react-native-iconfont-cli": "^2.2.4",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,md}": [
      "npm run prettier"
    ]
  }
}
